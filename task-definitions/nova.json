[
  {
    "name": "nova",
    "image": "096202052535.dkr.ecr.us-east-1.amazonaws.com/nova:latest",
    "cpu": 512,
    "memory": 512,
    "essential": true,
    "links": [
      "alexandria",
      "charon",
      "nova-queue"
    ],
    "entryPoint": [
      "java",
      "-jar",
      "/app/nova-assembly.jar",
      "--redis_host=nova-queue",
      "--redis_port=6379",
      "--ingest_max_interval=60",
      "--ingest_max_batch_size=60",
      "--alexandria_host=alexandria",
      "--alexandria_port=22045",
      "--charon_host=charon",
      "--charon_port=5609"
    ],
    "logConfiguration": {
      "logDriver": "awslogs",
      "options": {
        "awslogs-group": "asuna",
        "awslogs-region": "us-east-1",
        "awslogs-stream-prefix": "nova"
      }
    }
  },
  {
    "name": "queue",
    "image": "redis:3.2.5",
    "cpu": 512,
    "memory": 512,
    "essential": true,
    "logConfiguration": {
      "logDriver": "awslogs",
      "options": {
        "awslogs-group": "asuna",
        "awslogs-region": "us-east-1",
        "awslogs-stream-prefix": "nova-queue"
      }
    }
  }
]
