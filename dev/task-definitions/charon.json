[
  {
    "name": "charon",
    "image": "096202052535.dkr.ecr.us-east-1.amazonaws.com/charon",
    "cpu": 768,
    "memory": 3762,
    "essential": true,
    "entryPoint": [
      "java",
      "-Dlogback.configurationFile=logback.prod.xml",
      "-jar",
      "/app/charon-assembly.jar",
      "--riot_api_key=${riot_api_key}",
      "--req_per_10min=180000",
      "--req_per_10sec=3000",
      "--capacity_multiplier=0.8",
      "--stats_prefix=dev.charon",
      "--statsd_host=muramasa.dev",
      "--statsd_port=8125"
    ],
    "portMappings": [
      {
        "containerPort": 5609,
        "hostPort": 5609
      },
      {
        "containerPort": 5610,
        "hostPort": 5610
      }
    ],
    "logConfiguration": {
      "logDriver": "awslogs",
      "options": {
        "awslogs-group": "asuna",
        "awslogs-region": "us-east-1",
        "awslogs-stream-prefix": "charon"
      }
    }
  }
]
