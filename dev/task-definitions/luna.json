[
  {
    "name": "luna",
    "image": "096202052535.dkr.ecr.us-east-1.amazonaws.com/luna:latest",
    "cpu": 512,
    "memory": 1024,
    "essential": true,
    "entryPoint": [
      "java",
      "-Dlogback.configurationFile=logback.prod.xml",
      "-jar",
      "/app/luna-assembly.jar",
      "--alexandria_host=muramasa.dev",
      "--alexandria_port=22045",
      "--charon_host=muramasa.dev",
      "--charon_port=5609",
      "--stats_prefix=dev.luna",
      "--statsd_host=muramasa.dev",
      "--statsd_port=8125"
    ],
    "portMappings": [
      {
        "containerPort": 2389,
        "hostPort": 2389
      },
      {
        "containerPort": 2390,
        "hostPort": 2390
      }
    ],
    "logConfiguration": {
      "logDriver": "awslogs",
      "options": {
        "awslogs-group": "asuna",
        "awslogs-region": "us-east-1",
        "awslogs-stream-prefix": "luna"
      }
    }
  }
]
