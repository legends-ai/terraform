[
  {
    "name": "vulgate",
    "image": "096202052535.dkr.ecr.us-east-1.amazonaws.com/nova:latest",
    "cpu": 512,
    "memory": 512,
    "essential": true,
    "lins": [
      "charon",
      "vulgate-cache"
    ],
    "entryPoint": [
      "java",
      "-jar",
      "/app/vulgate-assembly.jar",
      "--charon_host=charon.dev",
      "--charon_port=5609",
      "--redis_host=vulgate-cache",
      "--redis_port=6379"
    ],
    "portMappings": [
      {
        "containerPort": 6205,
        "hostPort": 6205
      }
    ],
    "logConfiguration": {
      "logDriver": "awslogs",
      "options": {
        "awslogs-group": "asuna",
        "awslogs-region": "us-east-1",
        "awslogs-stream-prefix": "vulgate"
      }
    }
  },
  {
    "name": "cache",
    "image": "redis:3.2.5",
    "cpu": 512,
    "memory": 512,
    "essential": true,
    "logConfiguration": {
      "logDriver": "awslogs",
      "options": {
        "awslogs-group": "asuna",
        "awslogs-region": "us-east-1",
        "awslogs-stream-prefix": "vulgate"
      }
    }
  }
]
