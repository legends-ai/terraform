[
  {
    "name": "alexandria",
    "image": "096202052535.dkr.ecr.us-east-1.amazonaws.com/alexandria",
    "cpu": 512,
    "memory": 512,
    "essential": true,
    "entryPoint": [
      "java",
      "-jar",
      "/app/alexandria-assembly.jar",
      "--keyspace=alexandria_dev",
      "--cassandra_hosts=node-0.cassandra.dev"
    ],
    "portMappings": [
      {
        "containerPort": 22045,
        "hostPort": 22045
      },
      {
        "containerPort": 22046,
        "hostPort": 22046
      }
    ],
    "logConfiguration": {
      "logDriver": "awslogs",
      "options": {
        "awslogs-group": "asuna",
        "awslogs-region": "us-east-1",
        "awslogs-stream-prefix": "alexandria"
      }
    }
  }
]
