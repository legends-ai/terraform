[
  {
    "name": "alexandria",
    "image": "096202052535.dkr.ecr.us-east-1.amazonaws.com/alexandria",
    "cpu": 512,
    "memory": 10240,
    "essential": true,
    "entryPoint": [
      "java",
      "-Dlogback.configurationFile=logback.prod.xml",
      "-Dcom.sun.management.jmxremote.rmi.port=22047",
      "-Dcom.sun.management.jmxremote=true",
      "-Dcom.sun.management.jmxremote.port=22047",
      "-Dcom.sun.management.jmxremote.ssl=false",
      "-Dcom.sun.management.jmxremote.authenticate=false",
      "-Dcom.sun.management.jmxremote.local.only=false",
      "-Djava.rmi.server.hostname=localhost",
      "-jar",
      "/app/alexandria-assembly.jar",
      "--keyspace=alexandria_dev",
      "--c*_user=cassandra",
      "--c*_pass=cassandra",
      "--c*_pass=cassandra",
      "--c*_hosts=node-0.cassandra.dev,node-1.cassandra.dev,node-2.cassandra.dev",
      "--c*_replication_factor=3",
      "--lucinda_statistics_ttl=259200",
      "--lucinda_all_champion_statistics_ttl=259200",
      "--get_sum_parallelism=300",
      "--stats_prefix=dev.alexandria",
      "--statsd_host=muramasa.dev",
      "--statsd_port=8125"
    ],
    "portMappings": [
      {
        "containerPort": 22045,
        "hostPort": 22045
      },
      {
        "containerPort": 22046,
        "hostPort": 22046
      },
      {
        "containerPort": 22047,
        "hostPort": 22047
      }
    ],
    "logConfiguration": {
      "logDriver": "awslogs",
      "options": {
        "awslogs-group": "asuna",
        "awslogs-region": "us-east-1",
        "awslogs-stream-prefix": "alexandria"
      }
    }
  }
]
