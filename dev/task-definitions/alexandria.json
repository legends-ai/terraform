[
  {
    "name": "alexandria",
    "image": "096202052535.dkr.ecr.us-east-1.amazonaws.com/alexandria",
    "cpu": 512,
    "memory": 1024,
    "essential": true,
    "entryPoint": [
      "java",
      "-Dlogback.configurationFile=logback.prod.xml",
      "-jar",
      "/app/alexandria-assembly.jar",
      "--cassandra_user=cassandra",
      "--cassandra_pass=bitnami",
      "--keyspace=alexandria_dev",
      "--cassandra_hosts=node-0.cassandra.dev",
      "--stats_prefix=dev.alexandria",
      "--statsd_host=muramasa.dev",
      "--statsd_port=8125"
    ],
    "portMappings": [
      {
        "containerPort": 22045,
        "hostPort": 22045
      },
      {
        "containerPort": 22046,
        "hostPort": 22046
      }
    ],
    "logConfiguration": {
      "logDriver": "awslogs",
      "options": {
        "awslogs-group": "asuna",
        "awslogs-region": "us-east-1",
        "awslogs-stream-prefix": "alexandria"
      }
    }
  }
]
