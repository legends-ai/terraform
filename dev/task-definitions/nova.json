[
  {
    "name": "nova",
    "image": "096202052535.dkr.ecr.us-east-1.amazonaws.com/nova:latest",
    "cpu": 256,
    "memory": 1024,
    "essential": true,
    "links": [
      "queue"
    ],
    "entryPoint": [
      "java",
      "-jar",
      "/app/nova-assembly.jar",
      "--redis_host=queue",
      "--redis_port=6379",
      "--ingest_max_interval=60",
      "--ingest_max_batch_size=60",
      "--alexandria_host=muramasa.dev",
      "--alexandria_port=22045",
      "--charon_host=muramasa.dev",
      "--charon_port=5609",
      "--luna_host=muramasa.dev",
      "--luna_port=2389",
      "--stats_prefix=dev.nova",
      "--statsd_host=muramasa.dev",
      "--statsd_port=8125"
    ],
    "logConfiguration": {
      "logDriver": "awslogs",
      "options": {
        "awslogs-group": "asuna",
        "awslogs-region": "us-east-1",
        "awslogs-stream-prefix": "nova"
      }
    }
  },
  {
    "name": "queue",
    "image": "redis:3.2.5",
    "cpu": 256,
    "memory": 512,
    "essential": true,
    "logConfiguration": {
      "logDriver": "awslogs",
      "options": {
        "awslogs-group": "asuna",
        "awslogs-region": "us-east-1",
        "awslogs-stream-prefix": "nova"
      }
    }
  }
]
