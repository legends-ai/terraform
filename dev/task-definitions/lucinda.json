[
  {
    "name": "lucinda",
    "image": "096202052535.dkr.ecr.us-east-1.amazonaws.com/lucinda:latest",
    "cpu": 256,
    "memory": 512,
    "essential": true,
    "links": [
      "cache"
    ],
    "entryPoint": [
      "java",
      "-jar",
      "/app/lucinda-assembly.jar",
      "--alexandria_host=muramasa.dev",
      "--alexandria_port=22045",
      "--redis_host=cache",
      "--vulgate_host=muramasa.dev",
      "--vulgate_port=6205",
      "--stats_prefix=dev.lucinda",
      "--statsd_host=muramasa.dev",
      "--statsd_port=8125"
    ],
    "portMappings": [
      {
        "containerPort": 45045,
        "hostPort": 45045
      },
      {
        "containerPort": 45046,
        "hostPort": 45046
      }
    ],
    "logConfiguration": {
      "logDriver": "awslogs",
      "options": {
        "awslogs-group": "asuna",
        "awslogs-region": "us-east-1",
        "awslogs-stream-prefix": "lucinda"
      }
    }
  },
  {
    "name": "cache",
    "image": "redis:3.2.5",
    "cpu": 128,
    "memory": 512,
    "essential": true,
    "logConfiguration": {
      "logDriver": "awslogs",
      "options": {
        "awslogs-group": "asuna",
        "awslogs-region": "us-east-1",
        "awslogs-stream-prefix": "lucinda"
      }
    }
  }
]
