[
  {
    "name": "helios",
    "image": "096202052535.dkr.ecr.us-east-1.amazonaws.com/helios:latest",
    "cpu": 512,
    "memory": 512,
    "essential": true,
    "entryPoint": [
      "./helios",
      "--lucinda_host=lucinda.dev:45045",
      "--vulgate_host=vulgate.dev:6205"
    ],
    "portMappings": [
      {
        "containerPort": 7921,
        "hostPort": 7921
      },
      {
        "containerPort": 7922,
        "hostPort": 7922
      }
    ],
    "logConfiguration": {
      "logDriver": "awslogs",
      "options": {
        "awslogs-group": "asuna",
        "awslogs-region": "us-east-1",
        "awslogs-stream-prefix": "helios"
      }
    }
  }
]
